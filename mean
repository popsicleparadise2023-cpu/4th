mean1 = 87
wrongnumber = 65
correctnumber = 56
totalnumber = 40
# sum of 40 numbers
sum = mean1*totalnumber
print("The sum of fourty numbers is ", sum)

# correct sum of the numbers

num2=  sum-((wrongnumber)-(correctnumber))
print("sum-((wrongnumber)-(correctnumber)):", num2)

#the correct mean

mean2 = num2/totalnumber
print(mean2)
